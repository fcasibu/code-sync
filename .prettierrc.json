{
  "importOrder": [
    "^node(:.*)?$",
    "^(react$)|^(react/(.*)$)",
    "<THIRD_PARTY_MODULES>",
    "^@code-sync/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-prisma",
    "prettier-plugin-tailwindcss"
  ],
  "semi": true,
  "singleQuote": true
}
